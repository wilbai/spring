<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="CrmServiceConsumer"/>
    <dubbo:registry address="zookeeper://192.168.1.112:2181"/>

    <!--接收消费的服务-->
    <dubbo:reference interface="com.wil.crm.service.AccountService" id="accountService" version="1.0"/>
    <dubbo:reference interface="com.wil.crm.service.TaskService" id="taskService"/>
    <dubbo:reference interface="com.wil.crm.service.SaleChanceService" id="saleChanceService"/>
    <dubbo:reference interface="com.wil.crm.service.DiskService" id="diskService"/>
    <dubbo:reference interface="com.wil.crm.service.CustomerService" id="customerService"/>

</beans>